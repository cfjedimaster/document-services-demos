{
    "appenders": {
      "consoleAppender": {
        "_comment": "A sample console appender configuration, Clients can change as per their logging implementation",
        "type": "console",
        "layout": {
          "type": "pattern",
          "pattern": "%d:[%p]: %m"
        }
      }
    },
    "categories": {
      "default": {
        "appenders": [ "consoleAppender" ],
        "level": "off"
      }
    }
  }